INVOCATION_TYPE = COMPOSE

# Postgres configurations (do not modify)
mysql_DB=superset
mysql_HOST=mysql
mysql_PASS=root
mysql_USER=root

# Postgres engine specific environment variables
# change the below if you prefers another mysql engine
mysql_PORT=3306

# Add the mapped in /app/pythonpath_docker which allows devs to override stuff
PYTHONPATH=/app/pythonpath:/app/docker/pythonpath_dev


FLASK_ENV=production
SUPERSET_ENV=production
SUPERSET_LOAD_EXAMPLES=yes
CYPRESS_CONFIG=false
SUPERSET_PORT=8088
FORWARDED_ALLOW_IPS=*